<h1>User Test Index</h1>

<%= paginate @matches %>

<hr>
<% if current_user %>
      Signed in as <%= current_user.username %> (<%= link_to "Log out", session_path("current"), method: :delete %>)
    <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
    <% end %>
<hr>
<% @user.each do |user| %>
  <h3><%= user.username %></h3>
  <img src="<%=user.img%>" width="270px" height="170px">
  <h2><%= user.age %></h2>
  <h2><%= user.gender %></h2>
  <hr>
<% end %>